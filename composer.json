{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^2",
        "lexik/jwt-authentication-bundle": "^2.20",
        "symfony/console": "^7",
        "symfony/dotenv": "^7",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7",
        "symfony/http-client": "^7",
        "symfony/mailer": "^7",
        "symfony/mailgun-mailer": "^7",
        "symfony/proxy-manager-bridge": "^6",
        "symfony/security-bundle": "^7",
        "symfony/twig-bundle": "^7",
        "symfony/uid": "^7",
        "symfony/validator": "^7",
        "symfony/webhook": "^7",
        "symfony/webpack-encore-bundle": "^2",
        "symfony/yaml": "^7",
        "symfonycasts/verify-email-bundle": "^1.16",
        "twig/cssinliner-extra": "^3.8",
        "twig/extra-bundle": "^3.8"
    },
    "require-dev": {
        "symfony/debug-bundle": "^7",
        "symfony/maker-bundle": "^1",
        "symfony/var-dumper": "^7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7"
        }
    }
}
